{
  "name": "final",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky",
    "precommit": "lint-staged",
    "predev": "npm i",
    "prestart": "npm run build",
    "prebuild": "npm install",
    "test": "echo \"Error: no test specified\" && exit 0",
    "test:unit": "echo \"Error: no test specified\" && exit 0",
    "test:actions": "concurrently -g -n \"eslint,test:unit,build\" -c \"#341BAB,#A6CC66,#3077C6\" \"bun run lint\" \"bun run test:unit\" \"bun run build\"",
    "dev": "nodemon --watch src/index.js ",
    "start": "node dist/bundle.js",
    "deploy": "docker-compose -f docker-compose.prod.yaml up -d --build",
    "build": "webpack --config webpack.config.js --mode production",
    "lint": "eslint . -c .eslintrc.cjs --fix --cache",
    "format": "prettier --write ./**/*.{js,json,css}",
    "clean": "npm run format && npm run lint"
  },
  "lint-staged": {
    "*.js": [
      "npm run clean"
    ],
    "*.hbs": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylewandishin/CSCI3380-group-final.git"
  },
  "author": "kyle wandishin",
  "bugs": {
    "url": "https://github.com/kylewandishin/CSCI3380-group-final/issues"
  },
  "homepage": "https://github.com/kylewandishin/CSCI3380-group-final#readme",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-prettier": "^5.2.3",
    "handlebars-loader": "^1.7.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "bcryptjs": "^3.0.2",
    "express": "^4.21.2",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.1",
    "nodemon": "^3.1.9",
    "pg-promise": "^11.10.2"
  }
}
